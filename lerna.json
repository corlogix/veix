{
  "version": "1.0.2",
  "packages": [
    "packages/*"
  ],
  "compands": {
    "publish": {
      "conventionalCommits": true,
      "ignoreChanges": [
        "*.md",
        "*.sh"
      ],
      "message": "chore: published artifacts [skip ci]"
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  }
}
